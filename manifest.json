{
  "manifest_version": 3,
  "name": "VerbScrapper",
  "version": "1.0",
  "description": "Scrape verbs from",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "scripting"],
  "host_permissions": ["https://leconjugueur.lefigaro.fr/*"],
  "background": {
    "service_worker": "scripts/background.js"
  }
}
